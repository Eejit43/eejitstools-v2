<table class="info-table small-width">
    <thead>
        <tr>
            <th>Letter</th>
            <th>Code Word</th>
        </tr>
    </thead>
    <tbody>
        {{#each additionalData.phoneticAlphabet}}
            <tr>
                <td>{{this.letter}}</td>
                <td>{{this.codeWord}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
