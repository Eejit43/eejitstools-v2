<table class="info-table" style="overflow-x: hidden; margin-top: 30px">
    <thead>
        <tr>
            <th>Abbreviation</th>
            <th>Meaning</th>
        </tr>
    </thead>
    <tbody>
        {{#each additionalData.textAbbreviations}}
            <tr>
                <td>{{this.abbreviation}}</td>
                <td>{{{this.meaning}}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
